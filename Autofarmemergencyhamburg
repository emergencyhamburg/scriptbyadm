local function moveToPosition(vehicle, destination, speed)
    local distanceThreshold = 1
    while running do
        local currentPosition = vehicle.PrimaryPart.Position
        local distance = (destination - currentPosition).Magnitude
        if distance < distanceThreshold then
            print("Ziel erreicht:", destination)
            break
        end
        local direction = (destination - currentPosition).Unit
        local targetCFrame = CFrame.new(currentPosition + direction * speed * RunService.Heartbeat:Wait(), destination)
        vehicle:SetPrimaryPartCFrame(targetCFrame)
        vehicle.PrimaryPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
        vehicle.PrimaryPart.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
    end
end
local function autoFarm()
    local Character = LocalPlayer.Character
    local Humanoid = Character:FindFirstChildWhichIsA("Humanoid")
    if not Humanoid or not Humanoid.SeatPart or not Humanoid.SeatPart:IsA("VehicleSeat") then
        return
    end
    local SeatPart = Humanoid.SeatPart
    local Vehicle = SeatPart.Parent
    while running do
        for _, destination in ipairs(jobCoordinates[selectedJob] or {}) do
            if not running then break end
            print("Bewege zu:", destination)
            moveToPosition(Vehicle, destination, 120)
            wait(1)
        end
    end
    print("AutoFarm beendet.")
end
-- // Tabs \--
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({
    Name = "Ghost Logic ui AutoFarm v0.2",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "Ghost Logic",
    IntroEnabled = true,
    IntroText = "Welcome to the Beta 0.1"
})
local AutoFarmTab = Window:MakeTab({
    Name = "AutoFarm",
    Icon = "rbxassetid://4483362458",
    PremiumOnly = false
})
AutoFarmTab:AddDropdown({
    Name = "Select Job",
    Default = "Country Bus Driver",
    Options = {"Country Bus Driver", "City Bus Driver", "Autorob [SOON!]"},
    Callback = function(value)
        selectedJob = value
        print("AusgewÃ¤hlter Job:", selectedJob)
    end
})
AutoFarmTab:AddToggle({
    Name = "Enable AutoFarm",
    Default = false,
    Callback = function(state)
        running = state
        if running then
            task.spawn(autoFarm)
        end
    end
})
AutoFarmTab:AddToggle({
    Name = "Anti Fall Out",
    Default = false,
    Callback = function(state)
        isRunning = state
        if isRunning then
            task.spawn(function()
                while isRunning do
                    wait(0.1)
                    local vehicle = workspace.Vehicles:FindFirstChild(LocalPlayer.Name)
                    if vehicle then
                        local humanoid = LocalPlayer.Character:FindFirstChild("Humanoid")
                        if humanoid and not humanoid.SeatPart then
                            local driveSeat = vehicle:FindFirstChild("DriveSeat")
                            if driveSeat then
                                driveSeat:Sit(humanoid)
                            end
                        end
                    end
                end
            end)
        end
    end
})
-- // Update Log Tab \--
local UpdateLogTab = Window:MakeTab({
    Name = "Update Log / Info",
    Icon = "rbxassetid://4483362458",
    PremiumOnly = false
})
UpdateLogTab:AddSection({
    Name = "Version 0.01 - AutoFarm Tutorial"
})
UpdateLogTab:AddParagraph(
    "How does AutoFarm work?",
    [[
    1. Make sure you work for the Bus Company.
    2. Get a bus! Open the vehicle menu, select your bus and spawn it.
    3. Get in the bus. (You must be in the driver's seat!)
    4. Open the Script UI and enable "Enable AutoFarm".
    5. Optional: Enable "Anti Fall Out" to ensure you automatically get back into your bus in case of bugs.
    ]]
)
OrionLib:Init()
